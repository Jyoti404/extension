{
  "manifest_version": 3,
  "name": "Dark Background",
  "version": "1.0",
  "description": "Applies dark background to current page",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon16.png"
  },
  "permissions": ["activeTab", "scripting"]
}